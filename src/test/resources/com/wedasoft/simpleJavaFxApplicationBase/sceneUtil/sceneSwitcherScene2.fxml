<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.wedasoft.simpleJavaFxApplicationBase.sceneUtil.SceneSwitcherScene2Controller">
    <center>
        <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="scene2Label" text="This is scene2."/>
                <Separator prefWidth="200.0"/>
                <Button fx:id="switchToScene1Button" mnemonicParsing="false" onAction="#switchToScene1Action"
                        text="Switch to scene1"/>
                <Separator prefWidth="200.0"/>
                <Label text="Parameter value: n/a"/>
                <TextField fx:id="passedParameterTf"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </center>
</BorderPane>
